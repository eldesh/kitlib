_require "basis.smi"
_require "uref.sig"

structure URef (* : UREF *) =
struct
  type 'a uref (= boxed)
  val uref   : 'a -> 'a uref                   
  val equal  : 'a uref * 'a uref -> bool
  val !!     : 'a uref -> 'a
  val ::=    : 'a uref * 'a -> unit
  val unify  : ('a * 'a -> 'a) -> 'a uref * 'a uref -> unit
end

